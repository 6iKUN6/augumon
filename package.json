    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,vue,css,scss,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,vue,css,scss,json,md}\"",
