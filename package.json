    "@nuxt/content": "3.5.0",
    "@nuxt/icon": "1.12.0",
    "@nuxt/image": "1.10.0",
    "@nuxt/scripts": "0.11.6",
    "@nuxt/ui": "3.0.2",
    "@pinia/nuxt": "^0.11.0",
    "@unhead/vue": "^2.0.3",
    "@vueuse/core": "^13.1.0",
    "leafer-ui": "^1.6.1",
    "nuxt": "^3.15.4",
